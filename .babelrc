{
  "env": {
    "utils": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "modules": "commonjs",
            "targets": {
              "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
          }
        ]
      ],
      "plugins": [
        ["module-resolver", {
          "root": ["adber-ui"],
          "alias": {
            "@adber/adber-ui/src": "@adber/adber-ui/lib"
          }
        }]
      ]
    }
  }
}
